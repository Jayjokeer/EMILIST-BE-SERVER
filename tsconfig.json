{
    "compilerOptions": {
      "target": "ES6",
      "module": "commonjs",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "outDir": "./dist",
      "noEmit": false,
      "incremental": true,
      // "tsBuildInfoFile": "./.tsbuildinfo",
      "noEmitOnError": false, 
      "moduleResolution": "node"
      
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules"]
  }